#include <iostream>
#include<vector>
#include<algorithm>
using namespace std;
bool isprime(int n){
    int i,c=0;
    for(i=2;i*i<=n;i++){
        if(n%i==0)
        return false;
    }
    if(n!=1)
    return true;
    else
    return false;
}
int main() {
    int n;
    cin>>n;
    cout<<isprime(n);
    return 0;
}
output
3
1
