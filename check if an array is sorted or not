#include <iostream>
using namespace std;
bool issorted(int a[],int n){
    int i;
    for(i=1;i<n;i++){
        if(a[i]<a[i-1])
        return false;
    }
    return true;
}
int main() {
   int n;
   cin>>n;
   int a[n],i;
   for(i=0;i<n;i++){
       cin>>a[i];
   }
   cout<<issorted(a,n);
    return 0;
}
output
4
2 4 4 1
0
