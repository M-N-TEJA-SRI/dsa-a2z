#include <iostream>
#include<algorithm>
using namespace std;
bool dup(int a[],int n,int i){
   int j;
   for(j=0;j<n;j++){
       if(a[j]==i)
      return true;
   }
   return false;
}
int main() {
   int n,key;
   cin>>n>>key;
   int a[n],i;
   for(i=0;i<n;i++){
       cin>>a[i];
   }
   cout<<dup(a,n,key);
     
   
    return 0;
}
output
3 4 
2 0 4

1
