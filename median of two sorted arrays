#include <iostream>
using namespace std;
int main() {
   int n,m,i,j,c=0,tot,ind1,ind2,num1,num2;
   cin>>n>>m;
   int a[n],b[m];
   for(i=0;i<n;i++){
       cin>>a[i];
   }
   for(i=0;i<m;i++){
       cin>>b[i];
   }
   tot=(n+m);
   ind2=tot/2;
   ind1=ind2-1;
   i=0,j=0;
   while(i<n&&j<m){
       if(a[i]<b[j]){
           if(c==ind1)
           num1=a[i];
           if(c==ind2)
           num2=a[i];
           i++;
           c++;
       }
       else
       {
           if(c==ind1)
           num1=b[j];
           if(c==ind2)
           num2=b[j];
           j++;
           c++;
       }
   }
while(i<n){
    if(c==ind1)
           num1=a[i];
           if(c==ind2)
           num2=a[i];
           i++;
           c++;
}
while(j<m){
    if(c==ind1)
           num1=b[j];
           if(c==ind2)
           num2=b[j];
           j++;
           c++;
}
if(tot%2==1)
cout<<num2;
else
cout<<(num1+num2)/2;
    return 0;
}
output
2 4
2 7
3 4 5 6
4
