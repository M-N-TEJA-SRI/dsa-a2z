#include <iostream>
#include<algorithm>
#include<vector>
using namespace std;
int main() {
   int amount;
   cin>>amount;
   int a[]={1000,500,100,50,20,10,5,2,1};
   int c=0,i,n=9;
   for(i=0;i<9;i++){
       while(amount>=a[i]){
           amount-=a[i];
           c++;
       }
   }
   cout<<c;
    return 0;
}
output
2893
11
