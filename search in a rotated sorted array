#include <iostream>
using namespace std;
int search(int a[],int n,int key){
    int l=0,h=n-1,mid,ans=-1;
     while(l<=h){
      mid=(l+h)/2;
      if(a[mid]==key)
      {
         return mid;
      }
      if(a[l]<=a[mid]){
          if(a[l]<=key&&key<=a[mid])
          h=mid-1;
          else
          l=mid+1;
      }
      else if(a[mid]<=a[h]){
          if(a[mid]<=key&&key<=a[h])
          l=mid+1;
          else
          h=mid-1;
      }
  }
  return -1;
}

int main() {
   int n,key;
   cin>>n>>key;
   int a[n],i;
   for(i=0;i<n;i++){
       cin>>a[i];
   }
   cout<<search(a,n,key);
 
    return 0;
}
output
6 3
4 5 6 1 2 3
5
