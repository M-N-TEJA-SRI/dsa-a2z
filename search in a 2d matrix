#include <iostream>
#include<vector>
using namespace std;
bool search(vector<vector<int>>&a,int n,int m,int key){
    int l=0,h=(n*m)-1,mid,row,col;
    while(l<=h){
        mid=(l+h)/2;
        row=mid/m;
        col=mid%m;
        if(a[row][col]==key)
        return true;
        else if(a[row][col]>key)
        h=mid-1;
        else
        l=mid+1;
    }
    return false;
}
int main() {
  int n,m,i,j,key;
  cin>>n>>m>>key;
vector<vector<int>>a(n,vector<int>(m));
  for(i=0;i<n;i++){
      for(j=0;j<m;j++){
          cin>>a[i][j];
       }
  }
  cout<<search(a,n,m,key);
    return 0;
}
output
2 3 9 
1 2 3
4 7 8
0
