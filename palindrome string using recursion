#include <iostream>
using namespace std;
bool rec(string s,int i,int j){
    if(i<j){
        if(s[i]!=s[j])
        return false;
        else{
            i++;
            j--;
        return rec(s,i,j);
    }
    }
    return true;
}
int main() {
   string s;
   cin>>s;
   cout<<rec(s,0,s.length()-1);
    return 0;
}
output
atat
0
