#include <iostream>
using namespace std;
int lowerbound(int a[],int n,int key){
    int l=0,h=n-1,mid,ans=-1;
     while(l<=h){
      mid=(l+h)/2;
      if(a[mid]>=key)
      {
          ans=a[mid];
          h=mid-1;
      }
      else
      l=mid+1;
  }
  return ans;
}
int main() {
   int n,key;
   cin>>n>>key;
   int a[n],i;
   for(i=0;i<n;i++){
       cin>>a[i];
   }
 
  cout<<lowerbound(a,n,key);
    return 0;
}
output
6 7
1 2 3 4 5 10
10
