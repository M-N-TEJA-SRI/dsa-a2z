#include <iostream>
#include<algorithm>
using namespace std;
string odd(string s){
    int i;
    for(i=s.length()-1;i>=0;i--){
       if((s[i]-'0')%2==1)
       return s.substr(0,i+1);
    }
    return "";
    
}
int main() {
   string s;
   cin>>s;
   cout<<odd(s);

    return 0;
}
output
789348
7893
